Subject: [PATCH] Fix init property load

Change-Id: I12e75cd334105406f95313db7188b64bdaf6908e
Signed-off-by: schwienernitzel <pfelix0803@gmail.com>
---
 init/property_service.cpp | 9 ++++-----
 1 file changed, 4 insertions(+), 5 deletions(-)

diff --git a/init/property_service.cpp b/init/property_service.cpp
index fc14796..95a3232 100644
--- a/init/property_service.cpp
+++ b/init/property_service.cpp
@@ -575,6 +575,10 @@ static void load_persistent_properties() {
 
 void property_load_boot_defaults() {
     load_properties_from_file(PROP_PATH_RAMDISK_DEFAULT, NULL);
+    /* update with vendor-specific property runtime
+     * overrides
+     */
+    vendor_load_properties();
 }
 
 static void load_override_properties() {
@@ -640,11 +644,6 @@ void load_system_props() {
     load_properties_from_file(PROP_PATH_VENDOR_BUILD, NULL);
     load_properties_from_file(PROP_PATH_FACTORY, "ro.*");
 
-    /* update with vendor-specific property runtime
-     * overrides
-     */
-    vendor_load_properties();
-
     load_recovery_id_prop();
 }
 
